{
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "plugin:prettier/recommended",
    ],
    "rules": {
      "vue/multi-word-component-names": 0,
      "indent": [
        "error",
        2
      ],
      "vue-indent-script-and-style": 0,
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    }
  }